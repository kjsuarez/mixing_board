{
  "name": "Mixing Board",
  "description": "control volume on indevidual audio elements",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/48x48pond.png",
      "32": "/images/48x48pond.png",
      "48": "/images/48x48pond.png",
      "128": "/images/96x96pond.png"
    }
  },
  "icons": {
    "16": "/images/48x48pond.png",
    "32": "/images/48x48pond.png",
    "48": "/images/48x48pond.png",
    "128": "/images/96x96pond.png"
  }
}
